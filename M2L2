import discord
from discord.ext import commands
import random

intents = discord.Intents.default()
intents.message_content = True

bot = commands.Bot(command_prefix='$', intents=intents)

@bot.event
async def on_ready():
    print(f'We have logged in as {bot.user}')

@bot.command()
async def organik(ctx):
    await ctx.send(f'Contoh-contoh sampah organik: kulit pisang, inti appel, daging bekas, dll')

@bot.command()
async def anorganik(ctx):
    await ctx.send(f'Contoh-contoh sampah anorganik: botol plastik, botol kaca, kertas, dll')

bot.run("token")
